<div class="telehealth-button-bar">
    <button type="button" class="btn btn-default telehealth-btn telehealth-btn-notes d-none" aria-label="{{ "Toggle Notes"|xlt}}" title="{{ "Toggle Notes"|xlt}}" >
        <i class="fa fa-notes-medical fa-lg text-primary"></i>
    </button>
    <button type="button" class="btn btn-default telehealth-btn telehealth-btn-expand d-none" aria-label="{{ "Expand Window"|xlt}}" title="{{ "Expand Window"|xlt}}" >
        <i class="fa fa-expand fa-lg text-primary"></i>
    </button>
    <button type="button" class="btn btn-default telehealth-btn telehealth-btn-invite d-none" aria-label="{{ "Invite Participant"|xlt}}" title="{{ "Invite Participant"|xlt}}" >
        <i class="fa fa-user-plus fa-lg text-primary"></i>
    </button>
    <button type="button" class="btn btn-default telehealth-btn telehealth-btn-screenshare d-none" aria-label="{{ "Share Screen"|xlt}}" title="{{ "Share Screen"|xlt}}" >
        <i class="fa fa-laptop fa-lg text-primary"></i>
    </button>
    <button type="button" class="btn btn-default telehealth-btn telehealth-btn-video d-none" aria-label="{{ "Toggle Video"|xlt}}" title="{{ "Toggle Video"|xlt}}">
        <i class="fa fa-video fa-lg text-primary" data-enabled="true"></i>
    </button>
    <button type="button" class="btn btn-default telehealth-btn telehealth-btn-microphone d-none" aria-label="{{ "Toggle Microphone"|xlt}}" title="{{ "Toggle Microphone"|xlt}}">
        <i class="fa fa-microphone text-primary" data-enabled="true"></i>
    </button>
    <button type="button" class="btn btn-default telehealth-btn telehealth-btn-hangup d-none" aria-label="{{ "Hangup Call"|xlt}}" title="{{ "Hangup Call"|xlt}}" >
        <i class="fa fa-phone fa-lg fa-rotate-90 text-danger"></i>
    </button>
</div>