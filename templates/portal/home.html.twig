{% endblock %}

{% block header %}
	{% include "header.html.twig" %}
{% endblock %}

{% block content %}

	<h1>Content Area</h1>

	<ul>
		<li>User =
			{{user}}</li>
		<li>Where to =
			{{whereto}}</li>
		<li>Result =
			{{result}}</li>
		<li>MSGS =
			{{msgs}}</li>
		<li>MSG Count ={{msgcnt}}</li>
		<li>New Count =
			{{newcnt}}</li>
		<li>Globals =
			{{globals}}</li>
		<li>You Have =
			{{youHave}}</li>
		<li>JS Version =
			{{jsVersion}}</li>
	</ul>


{% endblock %}
