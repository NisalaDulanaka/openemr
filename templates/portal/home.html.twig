{% extends "base.html.twig" %}

{% block head %}
	{{parent()}}
	<script>
		var tab_mode = true;
		function restoreSession() { // dummy functions so the dlgopen function will work in the patient portal
		return true;
		}
		var isPortal = 1;
	</script>
	
	<script src="{{'../interface/main/tabs/js/dialog_utils.js?v=' ~ jsVersion}}"></script>
	<link href="{{'/portal/sign/css/signer_modal.css?v=' ~ jsVersion}}" rel="stylesheet"/>

	<script src="{{'/portal/sign/assets/signature_pad.umd.js?v=' ~ jsVersion}}"></script>
	<script src="{{'/portal/sign/assets/signer_api.js?v=' ~ jsVersion}}"></script>
{% endblock %}

{% block pagetitle %}
