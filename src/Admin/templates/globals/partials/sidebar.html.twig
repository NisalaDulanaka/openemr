<div class="col-xs-12 col-sm-2 sidebar sidebar-left sidebar-sm-show"> <!-- sidebar sidebar-left sidebar-sm-show -->
    <ul class="nav nav-pills nav-stacked" role="tablist" id="sidebarList">
        {% for m in menu if m in user_tabs or not user_mode %}
            <li role="presentation">
                <a href="#{{ m }}" aria-controls="{{ m }}" role="tab" data-toggle="pill">{{ m }}</a>
            </li>
        {% endfor %}
    </ul>
</div>
