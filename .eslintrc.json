{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jquery": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": ["error", 4],
        "no-console": "off",
        "no-unused-vars": "off",
        "no-alert":"off",
        "no-undef":"off",
        "no-param-reassign": "off",
        "no-inner-declarations": "off",
        "func-names":"off"
    }
}
